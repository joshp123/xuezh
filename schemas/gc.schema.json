{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "gc ok envelope",
  "type": "object",
  "required": [
    "ok",
    "schema_version",
    "command",
    "data",
    "artifacts",
    "truncated",
    "limits"
  ],
  "properties": {
    "ok": {
      "const": true
    },
    "schema_version": {
      "type": "string"
    },
    "command": {
      "const": "gc"
    },
    "data": {
      "type": "object",
      "required": [
        "dry_run",
        "apply",
        "candidates",
        "deleted_count",
        "bytes_freed"
      ],
      "properties": {
        "dry_run": {
          "type": "boolean"
        },
        "apply": {
          "type": "boolean"
        },
        "candidates": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "deleted_count": {
          "type": "integer"
        },
        "bytes_freed": {
          "type": "integer"
        }
      },
      "additionalProperties": true
    },
    "artifacts": {
      "type": "array"
    },
    "truncated": {
      "type": "boolean"
    },
    "limits": {
      "type": "object"
    }
  },
  "additionalProperties": true
}
