{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "srs.preview ok envelope",
  "type": "object",
  "required": [
    "ok",
    "schema_version",
    "command",
    "data",
    "artifacts",
    "truncated",
    "limits"
  ],
  "properties": {
    "ok": {
      "const": true
    },
    "schema_version": {
      "type": "string"
    },
    "command": {
      "const": "srs.preview"
    },
    "data": {
      "type": "object",
      "required": [
        "days",
        "forecast"
      ],
      "properties": {
        "days": {
          "type": "integer"
        },
        "forecast": {
          "type": "object",
          "properties": {
            "recall": {
              "type": "object"
            },
            "pronunciation": {
              "type": "object"
            }
          },
          "additionalProperties": true
        }
      },
      "additionalProperties": true
    },
    "artifacts": {
      "type": "array"
    },
    "truncated": {
      "type": "boolean"
    },
    "limits": {
      "type": "object"
    }
  },
  "additionalProperties": true
}
