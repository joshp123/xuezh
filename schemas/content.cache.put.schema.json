{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "content.cache.put ok envelope",
  "type": "object",
  "required": [
    "ok",
    "schema_version",
    "command",
    "data",
    "artifacts",
    "truncated",
    "limits"
  ],
  "properties": {
    "ok": {
      "const": true
    },
    "schema_version": {
      "type": "string"
    },
    "command": {
      "const": "content.cache.put"
    },
    "data": {
      "type": "object",
      "required": [
        "type",
        "key",
        "content_id"
      ],
      "properties": {
        "type": {
          "type": "string"
        },
        "key": {
          "type": "string"
        },
        "content_id": {
          "type": "string",
          "pattern": "^ct_[0-9a-f]{12}$"
        }
      },
      "additionalProperties": true
    },
    "artifacts": {
      "type": "array"
    },
    "truncated": {
      "type": "boolean"
    },
    "limits": {
      "type": "object"
    }
  },
  "additionalProperties": true
}
